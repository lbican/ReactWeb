{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json"]
    },
    "plugins": [
        "react",
        "only-warn"
    ],
    "rules": {
        "semi": [2, "always"],
        "@typescript-eslint/semi": ["off"],
        "@typescript-eslint/strict-boolean-expressions": "off"
    }
}
